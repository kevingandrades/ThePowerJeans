{
	"extends": "astro/tsconfigs/base",
	"include": [ ".astro/types.d.ts", "**/*" ],
	"exclude": [ "dist" ],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@assets/*": [
				"src/assets/*"
			],
			"@components": [
				"src/components/index"
			],
			"@sections": [
				"src/sections/index"
			],
			"@icons": [
				"src/icons/index"
			],
			"@consts": [
				"src/consts/index"
			]
		}
	}
}